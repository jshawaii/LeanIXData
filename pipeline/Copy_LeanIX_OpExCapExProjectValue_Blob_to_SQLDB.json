{
	"name": "Copy_LeanIX_OpExCapExProjectValue_Blob_to_SQLDB",
	"properties": {
		"description": "Copying contents from raw/ProjectCapExOpExOnly to SQLDB",
		"activities": [
			{
				"name": "Copy_Project_Fields_array_From_Blob_to_SQLDB_then_SQLDB",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": false,
							"wildcardFileName": "*.json",
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['data']['id']"
								},
								"sink": {
									"name": "ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data']['fields'][0]['name']"
								},
								"sink": {
									"name": "fields_type_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data']['fields'][0]['data']['value']"
								},
								"sink": {
									"name": "value",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['data']['fields']['data'"
					}
				},
				"inputs": [
					{
						"referenceName": "Output_LeanIX_CapEx_OpEx_ProjectsOnly_Json_Blob",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Output_LeanIX_Project_CapExOpExOnly",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}