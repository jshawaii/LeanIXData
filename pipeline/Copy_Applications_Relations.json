{
	"name": "Copy_Applications_Relations",
	"properties": {
		"description": "Iterative copy of each Application ID and calling the Application:Relations API and passing the ApplicationID as a parameter to retrieve relations data specific to the ApplicationID",
		"activities": [
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Copy data1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"isSequential": true
				}
			},
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Output_LeanIX_Project_JSON_to_BLOB",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "LeanIX Pipelines/Unit Testing Pipelines"
		},
		"annotations": []
	}
}