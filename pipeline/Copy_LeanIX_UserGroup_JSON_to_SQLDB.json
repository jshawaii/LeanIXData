{
	"name": "Copy_LeanIX_UserGroup_JSON_to_SQLDB",
	"properties": {
		"description": "Ingest LeanIX UserGroup.$.Data to SQLDB dbo.LeanIXUserGroup Table",
		"activities": [
			{
				"name": "Copy_LeanIXUserGroup_JSON",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true
						}
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['data'][0]['id']"
								},
								"sink": {
									"name": "id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['name']"
								},
								"sink": {
									"name": "name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['displayName']"
								},
								"sink": {
									"name": "displayName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['fullName']"
								},
								"sink": {
									"name": "fullname",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['type']"
								},
								"sink": {
									"name": "type",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['createdAt']"
								},
								"sink": {
									"name": "createdAt",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['updatedAt']"
								},
								"sink": {
									"name": "updatedAt",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['status']"
								},
								"sink": {
									"name": "status",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['level']"
								},
								"sink": {
									"name": "level",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['score']"
								},
								"sink": {
									"name": "score",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['rev']"
								},
								"sink": {
									"name": "rev",
									"type": "Int32"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "Output_LeanIX_UserGroup_JSON_Blob",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Output_LeanIX_UserGroup_SQLDB",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}