{
	"name": "CopyFromFileShareEarthPublic",
	"properties": {
		"description": "Copy files from \\\\earth\\public",
		"activities": [
			{
				"name": "CopyFromFileShareEarthPublic",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "FileServerReadSettings",
							"recursive": true,
							"wildcardFileName": "*.*"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "FileSharePictureFiles",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "PictureFile",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}